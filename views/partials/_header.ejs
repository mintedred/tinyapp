<header>
  <title>URL shortener</title>

  <% if (!user) { %>
    <ul>
      <li><a href="/register">Register</a></li>
      <li><a href="/login">Login</a></li>
    </ul>
  <% } else { %>
    <p>Hello <%= user %>!</p>
    <form action="/logout" method="POST">
      <input type="submit" value="Logout">
    </form>
    <hr/>
  <% } %>
    / <a href="/">Home</a> /
</header>
