<header>
 
  <% if (!email) { %>
    <ul>
      <li><a href="/register">Register</a></li>
      <li><a href="/login">Login</a></li>
    </ul>
  <% } else { %>
    <p><%= email %></p>
    <form action="/logout" method="POST">
      <input type="submit" value="Logout">
    </form>
  <% } %>
    <nav>
      <ul>
        <li><a href="/">Home</a></li>
        <li><a href="/urls/new">Shorten a URL</a></li>
    </nav>
</header>
